    "keywords": ["php", "container", "dependency-injection"],
    "homepage": "https://devop-studio.com",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "DevOp Studio",
            "email": "core@devop-studio.com",
            "homepage": "https://devop-studio.com"
        },
        {
            "name": "Zlatin Hristov",
